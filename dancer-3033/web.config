<?xml version="1.0"?>
<configuration>
  <system.web> 
    <compilation batch="false" />
  </system.web>
  <system.webServer>
    <handlers>
      <add name="iisnode" path="scripts.js" verb="*" modules="iisnode" />
    </handlers>
    <rewrite>
      <rules>
        <rule name="myapp">
          <match url="/*" />
          <action type="Rewrite" url="dist/server/server.js" />
        </rule>
      </rules>
    </rewrite>
  </system.webServer>
</configuration>